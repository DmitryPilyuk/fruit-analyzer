@namespace FruitComponent

<RadzenButton Variant="Variant.Filled" Visible=@visible Click=@onClick Text="Analyze!" ButtonStyle="ButtonStyle.Secondary" 
Style="border-radius: 15px; width: 30%; height: 15%; font-size: 200%; text-transform: uppercase; margin: 2%" />

@code {
	[Parameter, EditorRequired]
	public EventCallback<MouseEventArgs> onClick { get; set; }

	[Parameter, EditorRequired]
	public bool visible { get; set; } = true;
}
